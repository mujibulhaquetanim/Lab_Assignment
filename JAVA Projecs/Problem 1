import java.util.*;
public class main {
public static void main(String[] args) {

Scanner sc= new Scanner(System.in);
System.out.println("\n Enter The Number:");
int n=sc.nextInt();

for(int i=2; i<=n; i++) {
if(n%i==0) {
System.out.println("\n It is Not Prime Number: "+n);
break;
}
else {
System.out.println("\n It is A Prime Number: "+n);
break;
}
}


}

}
